<div class="rst-loginform">
  <a id="login-menu-button" href="#">LOGIN</a>
  <div class="div">
    <div class="rst-login">
    <%= form_for(resource, :as => resource_name, :url => session_path(:user)) do |f| %>
      <%= f.email_field :email, autofocus: true %>

      <%= f.password_field :password, autocomplete: "off" %>

      <%= f.submit "LOGIN" %>

      <!-- <p>Não é cadastrado? <%= link_to "Cadastrar-se", new_registration_path(resource_name) %></p> -->
      <p>Esqueceu sua senha? <%= link_to "Clique aqui", new_password_path(resource_name) %></p>
    <% end %>
  </div>
  </div>
</div>

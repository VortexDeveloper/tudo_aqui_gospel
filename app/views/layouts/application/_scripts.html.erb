<!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
<%= javascript_include_tag "jquery.1.11.1" %>
<%= javascript_include_tag "jquery_ujs" %>

<!-- Bootstrap Js Compiled Plugins -->
<%= javascript_include_tag "bootstrap.min" %>
<%= javascript_include_tag "bootstrap-tagsinput" %>
<%= javascript_include_tag "jqBootstrapValidation" %>

<!-- WoW Js -->
<%= javascript_include_tag "wow.min" %>

<!-- Add Fancybox -->
<%= javascript_include_tag "fancybox/jquery.fancybox" %>
<%= javascript_include_tag "fancybox/helpers/jquery.fancybox-media" %>

<!-- Owl Slider Js -->
<%= javascript_include_tag "owl.carousel" %>

<!-- Weather Js -->
<%= javascript_include_tag "jquery.simpleWeather" %>

<!-- Custome Selectbox Js -->
<%= javascript_include_tag "jquery.rs.selectbox" %>
<%= javascript_include_tag "main" %>
